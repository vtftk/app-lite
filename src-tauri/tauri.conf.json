{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "vtftk-lite",
  "version": "0.0.1",
  "identifier": "com.jacobtread.vtftk-lite",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "VTuber Fun ToolKit (VTFTK - Lite)",
        "width": 1100,
        "height": 625
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "../defaults/": "defaults/"
    },
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDVGQ0IwNjc2NEQ5ODQ1MTUKUldRVlJaaE5kZ2JMWCtjMmMxUm03Y05SNlhEZ1hRY2cyM1A3N2tFWmpENUpWclVvTEVUdERZekMK",
      "endpoints": [
        "https://github.com/vtftk/app-lite/releases/latest/download/latest.json"
      ]
    }
  }
}